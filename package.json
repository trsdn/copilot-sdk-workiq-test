{
  "name": "copilot-sdk-workiq-test",
  "version": "1.0.0",
  "description": "Testing GitHub Copilot SDK with Work IQ",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "agent": "npx tsx src/agent.ts",
    "news": "npx tsx src/news-app/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "copilot",
    "sdk",
    "workiq"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@github/copilot-sdk": "latest",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
